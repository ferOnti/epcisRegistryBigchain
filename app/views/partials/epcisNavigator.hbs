<!-- assets explorer -->
<div class="tab-pane fade" id="tab_navigator">

  <div class="row">
    <div class="col-sm-5">

      <div class="panel panel-success">
        <div class="panel-heading">
          <h2 class="panel-title">Epcis </h2>
        </div>

        <div class="panel-body">

            <table id="table-sold" class="table table-striped table-bordered col-sm-6">
              <thead>
                <tr>
                  <th>epcid</th>
                  <th> </th>
                </tr>
              </thead>
              <tbody id="tbody-sold">

              </tbody>
            </table>
          </div>
        </div>
    </div>

    <div class="col-sm-7">

      <div class="panel panel-success">
        <div class="panel-heading">
          <h2 class="panel-title">Provenance</h2>
        </div>
          <div class="form-group">
            <pre><code id="sold-result" class="xml"><br><br></code></pre>
          </div>
      </div>
    </div>
  </div>
</div>

<script id="soldList-template" type="text/x-handlebars-template">
    \{{#each epcisList}}
    <tr>
      <td>\{{epcid}}</td>
      <td><button class="btn btn-default" onclick="epcidNav('\{{epcid}}'); "></button></td>
    </tr>
    \{{/each}}
</script>



